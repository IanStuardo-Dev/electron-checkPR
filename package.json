{
  "name": "electron-checkpr",
  "version": "1.0.0",
  "description": "Azure PR Review Desktop App",
  "main": "dist/main.js",
  "scripts": {
    "start": "concurrently \"webpack serve --mode development\" \"wait-on http://localhost:8080 && electron .\"",
    "build": "webpack --mode production && tsc",
    "watch": "tsc -w",
    "dev": "NODE_ENV=development npm run start",
    "build:prod": "NODE_ENV=production electron-builder build",
    "test:prod": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "analyze:complexity": "cr src/",
    "analyze:duplicates": "jscpd src/",
    "analyze:security": "eslint . --ext .ts,.tsx --config .eslintrc.security.js",
    "analyze:all": "npm run lint && npm run analyze:complexity && npm run analyze:duplicates && npm run analyze:security"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "autoprefixer": "^10.4.17",
    "complexity-report": "^2.0.0-alpha",
    "concurrently": "^8.2.2",
    "css-loader": "^6.9.0",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^2.1.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jscpd": "^3.5.9",
    "postcss": "^8.4.33",
    "postcss-loader": "^7.3.4",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "wait-on": "^7.2.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.1.1",
    "framer-motion": "^10.18.0",
    "react": "^18.2.0",
    "react-diff-viewer-continued": "^3.4.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "recharts": "^2.15.1"
  }
}
